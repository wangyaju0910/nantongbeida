Page{
  background-color: #f5f5f5;
}
.take-address{
  background-color: #fff;
  margin-top: 30rpx;
  font-size: 25rpx;
  padding: 30rpx 20rpx;
}
.address-basic view:nth-child(2){
  color:rgb(102, 102, 102);
  margin: 10rpx 0;
}
.address-basic view:nth-child(3){
  color:rgb(102, 102, 102);
}
.address-xuanze{
  display: flex;
  margin-top: 30rpx;
}
.xuanze{
  flex: 1;
  display: flex;
}
.xuanze .xuanze-img{
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}
.xuanze-img image{
  width: 100%;
  height: 100%;
}
.compile .compile-img{
   width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.compile-img image{
  width: 100%;
  height: 100%;
}
.compile{
  display: flex;
}
.compile:nth-child(1){
  margin-right: 30rpx;
}
.compile-delete{
  display: flex;
  color:#999;
}
.add-address{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 80rpx;
  background-color: rgb(588, 84, 84);
  color:#fff;
  text-align: center;
  line-height: 80rpx;
  font-size: 30rpx;
}