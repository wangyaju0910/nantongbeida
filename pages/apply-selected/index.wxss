Page{
  background-color: #f5f5f5;
}
.apply-enter{
  font-size: 25rpx;
  color:#999;
  padding: 20rpx;
}
.apply-enter .apply-title{
  font-size: 30rpx;
  color:#000;
  text-align: center;
  margin: 10rpx 0 20rpx 0;
}
.apply-enter .content-num{
  margin-bottom: 30rpx;
}
.consent{
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #fff;
  width: 100%;
  font-size: 20rpx;
}
.consent .consent-select{
  padding: 20rpx 0;
  color:rgb(51, 51, 51);
  display: flex;
}
.consent-select .select-img{
  width: 30rpx;
  height: 30rpx;
  margin: 0 10rpx;
}
.select-img image{
  width: 100%;
  height: 100%;
}
.consent .content-button{
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  background-color: rgb(255, 85, 85);
  color:#fff;
}