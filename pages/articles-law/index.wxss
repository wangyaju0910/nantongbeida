Page {
  background-color: #f5f5f5;
}

.articles-law {
  font-size: 30rpx;
  background-color: #fff;
  margin-top: 30rpx;
}

.articles-list {
  display: flex;
  padding: 30rpx 20rpx;
  border-bottom: 1px solid #f5f5f5;
}
.articles-list>view:nth-child(1){
  flex: 1;
}
.articles-img {
  width: 17rpx;
  height: 30rpx;
}

.articles-img image {
  width: 100%;
  height: 100%;
}
