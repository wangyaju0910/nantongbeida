  <view class='skip-list' bindtap='noEvaluate'>
    <view class='list'>
      <view>
        <view class='list-img'>
          <image src='../../images/wodepingjia@2x.png'></image>
        </view>
        <view class='list-content'>我的评价</view>
      </view>
      <view class='gengduo-img'>
