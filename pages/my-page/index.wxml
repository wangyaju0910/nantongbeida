        <view class='list-content'>法律条款</view>
      </view>
      <view class='gengduo-img'>
        <image src='../../images/gengduo@2x.png'></image>
      </view>
    </view>
  </view>
</view>
<view class='special-button'>
  <view class='customization'>
    <image src='../../images/wodekefu.png'></image>
