.region{
  /* display: flex; */
  background-color: #f5f5f5;
  padding: 25rpx 10rpx;
}
.region-list{
  width: 100%;
  margin-bottom: 20rpx;
}
.region-list .region-img{
  width: 95%;
  height: 250rpx;
  margin: 0 auto;
  position: relative;
}
.region-img image{
  width: 100%;
  height: 100%;
  border-radius: 15rpx;

}
.region-content{
  width: 90%;
  position: absolute;
  top: 30%;
  left: 5%;
  display: flex;
  font-size: 30rpx;
  color:#fff;
}
.region-title{
  flex: 15;
}
.region-go{
  flex: 1;
  font-size: 50rpx;
  margin-top: 16rpx;
}
.title{
  font-size: 40rpx;
}







.tabBar {
  font-size: 30rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  display: flex;
  background-color: #fff;
  padding: 10rpx 0;
  color: #999;
}

.tabBar .tabBar-list {
  flex: 1;
  text-align: center;
}

.tabBar .tabBar-list:nth-child(3) {
  color: rgb(255, 85, 85);
}

.tabBar .tabBar-img {
  width: 40rpx;
  height: 40rpx;
  margin: 10rpx auto;
}

.tabBar-img image {
  width: 100%;
  height: 100%;
}
.tabBar-img .yuandian{
  width: 10rpx;
  height: 10rpx;
  background-color: rgb(255, 85, 85);
  float: right;
  border-radius: 50%;
}