.submint{
  font-size: 30rpx;
  margin: 200rpx auto;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.submint .submint-img{
  width: 120rpx;
  height: 110rpx;
  margin-bottom: 40rpx;
}
.submint-img image{
  width: 100%;
  height: 100%;
}
.wait{
  margin: 20rpx 0;
}
.submit-back{
  border: 1px solid rgb(255, 85, 85);
  color: rgb(255, 85, 85);
  padding: 15rpx 100rpx;
  margin-top: 50rpx;
  border-radius: 10rpx;
}